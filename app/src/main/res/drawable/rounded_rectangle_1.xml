<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- rectangle表示为矩形 -->

    <!-- 填充的颜色 -->
    <solid android:color="#00000000" />

    <!-- 边框的颜色和粗细 -->
    <stroke
        android:width="2px"
        android:color="#FFFFFF"
        />

    <!-- android:radius 圆角的半径 -->
    <corners
        android:radius="400px"
        />

</shape>
